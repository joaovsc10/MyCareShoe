<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="270dp"
            android:layout_height="268dp"
            android:layout_gravity="center"
            android:src="@drawable/upperfoot"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="0dp" />

        <ImageView
            android:id="@+id/metatarsal2_r_upper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginBottom="7dp"
            android:src="@drawable/ic_baseline_circle"
            app:layout_constraintBottom_toBottomOf="@+id/metatarsal1_r_upper"
            app:layout_constraintEnd_toStartOf="@+id/metatarsal1_r_upper" />

        <ImageView
            android:id="@+id/metatarsal1_r_upper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="87dp"
            android:layout_marginLeft="87dp"
            android:layout_marginBottom="110dp"
            android:src="@drawable/ic_baseline_circle"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3"
            app:layout_constraintStart_toStartOf="@+id/imageView3" />

        <ImageView
            android:id="@+id/metatarsal3_r_upper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginBottom="7dp"
            android:src="@drawable/ic_baseline_circle"
            app:layout_constraintBottom_toBottomOf="@+id/metatarsal2_r_upper"
            app:layout_constraintEnd_toStartOf="@+id/metatarsal2_r_upper" />

        <ImageView
            android:id="@+id/dorsal_r_upper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginBottom="35dp"
            android:src="@drawable/ic_baseline_circle"
            app:layout_constraintBottom_toTopOf="@+id/metatarsal2_r_upper"
            app:layout_constraintStart_toStartOf="@+id/metatarsal2_r_upper" />


        <ImageView
            android:id="@+id/metatarsal1_l_upper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:src="@drawable/ic_baseline_circle"
            app:layout_constraintStart_toEndOf="@+id/metatarsal1_r_upper"
            app:layout_constraintTop_toTopOf="@+id/metatarsal1_r_upper" />

        <ImageView
            android:id="@+id/metatarsal2_l_upper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="7dp"
            android:src="@drawable/ic_baseline_circle"
            app:layout_constraintBottom_toBottomOf="@+id/metatarsal1_l_upper"
            app:layout_constraintStart_toEndOf="@+id/metatarsal1_l_upper" />


        <ImageView
            android:id="@+id/metatarsal3_l_upper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="7dp"
            android:src="@drawable/ic_baseline_circle"
            app:layout_constraintBottom_toBottomOf="@+id/metatarsal2_l_upper"
            app:layout_constraintStart_toEndOf="@+id/metatarsal2_l_upper" />

        <ImageView
            android:id="@+id/dorsal_l_upper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginBottom="35dp"
            android:src="@drawable/ic_baseline_circle"
            app:layout_constraintBottom_toTopOf="@+id/metatarsal2_l_upper"
            app:layout_constraintStart_toStartOf="@+id/metatarsal2_l_upper" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>